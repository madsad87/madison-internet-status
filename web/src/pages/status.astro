---
const title = "Internet Status";
const apiBaseUrl = import.meta.env.PUBLIC_API_BASE_URL ?? import.meta.env.API_BASE_URL ?? "";
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
    <link rel="icon" href="/favicon.svg" type="image/svg+xml" />
    <style>
      :root {
        --color-black: #2b241b;
        --color-primary: #8b5e3c;
        --color-secondary: #b07a53;
        --color-tertiary: #d9c7b2;
        --color-white: #fbf7f0;
        --color-background: #e7dccb;
        --color-surface: #f3eadb;
        --color-muted: #6f6153;
        --color-accent: #6b4c34;
        --color-border: #d2c1ac;
        --color-score-ring: #6fa86f;
        --color-score-bg: #dcead3;
      }

      * {
        box-sizing: border-box;
      }

      body {
        margin: 0;
        font-family: Inter, Verdana, Arial, Helvetica, sans-serif;
        font-weight: 400;
        letter-spacing: 0.01em;
        line-height: 1.6;
        background: var(--color-background);
        color: var(--color-black);
      }

      main {
        max-width: 1200px;
        margin: 0 auto;
        padding: 4rem 1.5rem 6rem;
      }

      header {
        display: flex;
        flex-direction: column;
        gap: 1rem;
        margin-bottom: 2.5rem;
      }

      h1 {
        margin: 0;
        font-size: clamp(2.1rem, 3vw, 3.2rem);
      }

      .subhead {
        color: var(--color-muted);
        max-width: 52ch;
      }

      .status-banner {
        display: flex;
        flex-wrap: wrap;
        align-items: center;
        gap: 0.75rem 1.2rem;
        padding: 0.8rem 1.2rem;
        border-radius: 1.2rem;
        background: var(--color-surface);
        border: 1px solid var(--color-border);
        box-shadow: 0 10px 22px rgba(46, 34, 24, 0.08);
        font-size: 0.95rem;
      }

      .status-banner strong {
        font-weight: 600;
        color: var(--color-accent);
      }

      .status-banner-item {
        display: flex;
        align-items: center;
        gap: 0.45rem;
        min-width: 0;
        flex-wrap: wrap;
      }

      .status-banner-item .meta {
        margin: 0;
      }

      .grid {
        display: grid;
        gap: 1.8rem;
        grid-template-columns: repeat(3, minmax(260px, 1fr));
      }

      @media (max-width: 1024px) {
        .grid {
          grid-template-columns: repeat(2, minmax(240px, 1fr));
        }
      }

      @media (max-width: 640px) {
        .grid {
          grid-template-columns: 1fr;
        }
      }

      .card {
        background: var(--color-surface);
        border: 1px solid var(--color-border);
        border-radius: 2rem;
        padding: 2rem 2.2rem;
        box-shadow: 0 18px 36px rgba(46, 34, 24, 0.08);
        transition: transform 0.2s ease, box-shadow 0.2s ease;
      }

      .card:hover {
        transform: translateY(-1px);
        box-shadow: 0 22px 40px rgba(46, 34, 24, 0.12);
      }

      .card h2 {
        margin: 0 0 1rem;
        font-size: 1.3rem;
      }

      .metric {
        font-variant-numeric: tabular-nums;
        font-size: 2.1rem;
        margin: 0;
      }

      .meta {
        color: var(--color-muted);
        font-size: 0.95rem;
      }

      .status-pill {
        display: inline-flex;
        align-items: center;
        gap: 0.5rem;
        padding: 0.3rem 0.9rem;
        border-radius: 999px;
        font-weight: 600;
        font-size: 0.85rem;
        text-transform: uppercase;
        letter-spacing: 0.06em;
        background: var(--color-tertiary);
        color: var(--color-accent);
      }

      .status-pill.ok {
        background: #dfe9d5;
        color: #44512e;
      }

      .status-pill.warn {
        background: #f2e1c5;
        color: #7b5a2c;
      }

      .status-pill.down {
        background: #f2c7c7;
        color: #7c3434;
      }

      .sparkline {
        width: 100%;
        height: 56px;
        margin-top: 1rem;
      }

      .metrics-grid {
        display: grid;
        grid-template-columns: repeat(2, minmax(0, 1fr));
        gap: 1.5rem;
        background: var(--color-surface);
        padding: 1.5rem;
        border-radius: 1.5rem;
      }

      .metric-item {
        display: flex;
        flex-direction: column;
        align-items: center;
        color: var(--color-black);
      }

      .metric-circle {
        width: 84px;
        height: 84px;
        border-radius: 50%;
        border: 6px solid var(--color-score-ring);
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 1.5rem;
        font-weight: 600;
        margin-bottom: 0.5rem;
        background: var(--color-score-bg);
      }

      .metric-label {
        font-size: 0.9rem;
        color: var(--color-muted);
        text-align: center;
      }

      .list {
        list-style: none;
        padding: 0;
        margin: 1rem 0 0;
        display: grid;
        gap: 0.75rem;
      }

      .list li {
        display: flex;
        flex-direction: column;
        gap: 0.15rem;
      }

      .list a {
        color: var(--color-accent);
        text-decoration: none;
        font-weight: 600;
      }

      .list a:hover {
        text-decoration: underline;
      }

      .button {
        display: inline-flex;
        align-items: center;
        justify-content: center;
        padding: 1.4rem 2.8rem;
        border-radius: 999px;
        border: 1px solid var(--color-primary);
        background: var(--color-primary);
        color: var(--color-white);
        font-weight: 600;
        cursor: pointer;
        box-shadow: 0 12px 24px rgba(46, 34, 24, 0.15);
        transition: background 0.2s ease, transform 0.2s ease;
      }

      .button:hover {
        background: var(--color-accent);
        transform: translateY(-1px);
      }

      .terminal-card {
        background-color: #2d231a;
        color: #f5e9d7;
        padding: 2.4rem;
        border-radius: 2rem;
        font-family: SFMono-Regular, Consolas, Liberation Mono, Menlo, monospace;
        box-shadow: 0 24px 50px rgba(46, 34, 24, 0.25);
      }

      .terminal-card h2 {
        margin-top: 0;
      }

      .terminal-lines {
        margin: 1.2rem 0 0;
        display: grid;
        gap: 0.6rem;
        font-size: 0.95rem;
      }

      .modal {
        position: fixed;
        inset: 0;
        background: rgba(43, 36, 27, 0.45);
        display: none;
        align-items: center;
        justify-content: center;
        padding: 2rem;
        z-index: 20;
      }

      .modal.open {
        display: flex;
      }

      .modal-panel {
        background: var(--color-surface);
        border-radius: 2rem;
        border: 1px solid var(--color-border);
        padding: 2rem;
        width: min(720px, 100%);
        box-shadow: 0 24px 48px rgba(46, 34, 24, 0.18);
      }

      .modal-header {
        display: flex;
        align-items: center;
        justify-content: space-between;
        gap: 1rem;
      }

      .modal-body {
        margin-top: 1rem;
      }

      .modal-chart {
        width: 100%;
        height: 120px;
      }

      .loading {
        opacity: 0.6;
      }

      .site-header {
        color: var(--color-black);
        background-color: var(--color-surface);
        border-bottom: 1px solid var(--color-border);
        font-family: "Inter", "Avenir Next", "Segoe UI", sans-serif;
      }

      .site-header .container {
        max-width: 1200px;
        margin: 0 auto;
        padding: 0 1.5rem;
      }

      .site-header .bar {
        display: flex;
        justify-content: space-between;
        flex-wrap: wrap;
        align-items: center;
        padding: 1.5rem 0;
        gap: 2rem;
      }

      .site-header .logo a {
        display: inline-flex;
        align-items: center;
        gap: 1rem;
        text-decoration: none;
        color: inherit;
      }

      .site-header .logo-mark {
        display: inline-flex;
        align-items: center;
        justify-content: center;
        width: 3.2rem;
        height: 3.2rem;
        border-radius: 1.1rem;
        background-color: rgba(120, 84, 58, 0.12);
        border: 1px solid rgba(120, 84, 58, 0.2);
        color: var(--color-primary);
        font-size: 1.5rem;
      }

      .site-header .logo-text {
        font-size: 1.125rem;
        font-weight: 600;
        letter-spacing: 0.02em;
        color: var(--color-black);
      }

      .site-header .logo-dot {
        color: var(--color-primary);
      }

      .sr-only {
        border: 0;
        clip: rect(0 0 0 0);
        height: 1px;
        margin: -1px;
        overflow: hidden;
        padding: 0;
        position: absolute;
        width: 1px;
      }

      .nav-toggle {
        display: flex;
        align-items: center;
        margin: 0;
        padding: 1.2rem;
        background-color: transparent;
        color: var(--color-black);
        border: 0.1rem solid var(--color-border);
        transition: color 0.3s ease, background-color 0.3s ease;
      }

      .nav-toggle:hover,
      .nav-toggle:focus {
        background-color: var(--color-primary);
        color: var(--color-white);
      }

      @media (min-width: 720px) {
        .nav-toggle {
          display: none;
        }
      }

      .primary-navigation {
        display: none;
        width: 100%;
        margin-left: auto;
      }

      .primary-navigation.show {
        display: block;
      }

      @media (min-width: 720px) {
        .primary-navigation {
          display: flex;
          width: auto;
          justify-content: flex-end;
        }
      }

      .primary-navigation .menu {
        display: flex;
        flex-direction: column;
        align-items: flex-start;
        list-style: none;
        text-transform: uppercase;
        font-size: 0.875rem;
        letter-spacing: 0.08em;
        margin: 0;
        padding: 0;
        gap: 0.75rem;
      }

      @media (min-width: 720px) {
        .primary-navigation .menu {
          flex-direction: row;
          gap: 1.8rem;
          justify-content: flex-end;
          width: 100%;
        }
      }

      .primary-navigation .menu li {
        margin: 0;
      }

      .primary-navigation .menu a {
        display: inline-flex;
        align-items: center;
        height: 100%;
        padding: 0.6rem 0.6rem;
        font-weight: 600;
        color: var(--color-black);
        text-decoration: none;
      }

      .primary-navigation .menu a:hover,
      .primary-navigation .menu a:focus {
        border: 1px solid var(--color-border);
        border-radius: 999px;
        color: var(--color-primary);
        padding: 0.5rem 1rem;
      }

      .nav-icon {
        width: 1.1rem;
        height: 1.1rem;
        display: inline-block;
      }
    </style>
  </head>
  <body>
    <header class="site-header">
      <div class="container">
        <div class="bar">
          <div class="logo">
            <a href="/" title="Home">
              <span class="logo-mark" aria-hidden="true">
                <svg class="nav-icon" viewBox="0 0 24 24" role="img" aria-hidden="true">
                  <path
                    fill="currentColor"
                    d="M8.7 6.1a1 1 0 0 1 0 1.4L5.2 11l3.5 3.5a1 1 0 1 1-1.4 1.4l-4.2-4.2a1 1 0 0 1 0-1.4l4.2-4.2a1 1 0 0 1 1.4 0zm6.6 0a1 1 0 0 1 1.4 0l4.2 4.2a1 1 0 0 1 0 1.4l-4.2 4.2a1 1 0 0 1-1.4-1.4L18.8 11l-3.5-3.5a1 1 0 0 1 0-1.4z"
                  />
                </svg>
              </span>
              <span class="logo-text">
                madison<span class="logo-dot">.status</span>
              </span>
              <span class="sr-only">Madison Status</span>
            </a>
          </div>
          <button
            type="button"
            class="nav-toggle"
            aria-label="Toggle navigation"
            aria-controls="primary-navigation"
            aria-expanded="false"
            id="nav-toggle"
          >
            <svg class="nav-icon" viewBox="0 0 24 24" role="img" aria-hidden="true">
              <path
                fill="currentColor"
                d="M3 6.5c0-.55.45-1 1-1h16a1 1 0 1 1 0 2H4c-.55 0-1-.45-1-1zm0 5c0-.55.45-1 1-1h16a1 1 0 1 1 0 2H4c-.55 0-1-.45-1-1zm1 4c-.55 0-1 .45-1 1s.45 1 1 1h16a1 1 0 1 1 0 2H4c-.55 0-1-.45-1-1s.45-1 1-1h16a1 1 0 1 1 0 2H4z"
              />
            </svg>
          </button>
          <nav class="primary-navigation" id="primary-navigation">
            <ul class="menu">
              <li><a href="https://madisonsadler.com" target="_blank" rel="noreferrer">Portfolio</a></li>
              <li><a href="https://madisonsadler.com/posts" target="_blank" rel="noreferrer">Posts</a></li>
              <li><a href="https://github.com/madsad87" target="_blank" rel="noreferrer">GitHub</a></li>
              <li>
                <a href="/search" aria-label="Search">
                  <svg class="nav-icon" viewBox="0 0 24 24" role="img" aria-hidden="true">
                    <path
                      fill="currentColor"
                      d="M10.5 3a7.5 7.5 0 0 1 5.93 12.07l3.75 3.75a1 1 0 0 1-1.41 1.41l-3.75-3.75A7.5 7.5 0 1 1 10.5 3zm0 2a5.5 5.5 0 1 0 0 11a5.5 5.5 0 0 0 0-11z"
                    />
                  </svg>
                </a>
              </li>
            </ul>
          </nav>
        </div>
      </div>
    </header>
    <main>
      <header>
        <h1>Internet status</h1>
        <div class="status-banner" aria-live="polite">
          <div class="status-banner-item">
            <span class="meta">Overall</span>
            <span class="status-pill" id="overall-status-pill">Loading</span>
          </div>
          <div class="status-banner-item">
            <span class="meta">Last updated</span>
            <strong id="overall-updated">--</strong>
          </div>
          <div class="status-banner-item">
            <span class="meta">Incidents</span>
            <strong id="overall-incidents">--</strong>
          </div>
        </div>
        <p class="subhead">
          A quiet dashboard that mirrors madisonsadler.com — checking uptime, performance, and the
          latest updates in one calm place.
        </p>
      </header>

      <section class="grid" id="status-grid">
        <article class="card" data-modal-key="uptime:site" aria-labelledby="uptime-title">
          <h2 id="uptime-title">Website Uptime</h2>
          <div class="status-pill" id="uptime-status" role="status" aria-live="polite">Loading</div>
          <p class="metric" id="uptime-latency">--</p>
          <p class="meta" id="uptime-meta">Last check: --</p>
          <svg class="sparkline" id="uptime-sparkline" viewBox="0 0 120 40"></svg>
        </article>

        <article class="card" aria-labelledby="lighthouse-title">
          <h2 id="lighthouse-title">Lighthouse Scores</h2>
          <div class="metrics-grid">
            <div class="metric-item">
              <div class="metric-circle">100</div>
              <div class="metric-label">Performance</div>
            </div>
            <div class="metric-item">
              <div class="metric-circle">90</div>
              <div class="metric-label">Accessibility</div>
            </div>
            <div class="metric-item">
              <div class="metric-circle">100</div>
              <div class="metric-label">Best Practices</div>
            </div>
            <div class="metric-item">
              <div class="metric-circle">92</div>
              <div class="metric-label">SEO</div>
            </div>
          </div>
        </article>

        <article class="card" aria-labelledby="github-title">
          <h2 id="github-title">GitHub Activity</h2>
          <p class="meta" id="github-meta">Loading…</p>
          <ul class="list" id="github-list"></ul>
        </article>

        <article class="card" aria-labelledby="posts-title">
          <h2 id="posts-title">Latest Posts</h2>
          <p class="meta" id="posts-meta">Loading…</p>
          <ul class="list" id="posts-list"></ul>
        </article>

        <article class="card" aria-labelledby="now-title">
          <h2 id="now-title">Currently…</h2>
          <p class="metric" id="now-text" role="status">Loading…</p>
          <p class="meta" id="now-updated">--</p>
        </article>

        <article class="terminal-card" aria-labelledby="incident-title">
          <h2 id="incident-title">Incident Log</h2>
          <div class="terminal-lines" id="incident-log">
            <div>Awaiting signal…</div>
          </div>
        </article>
      </section>
    </main>

    <div class="modal" id="history-modal" aria-hidden="true">
      <div class="modal-panel">
        <div class="modal-header">
          <h3 id="modal-title">History</h3>
          <button class="button" id="modal-close" type="button">Close</button>
        </div>
        <div class="modal-body">
          <svg class="modal-chart" id="modal-chart" viewBox="0 0 300 120"></svg>
          <div class="meta" id="modal-meta"></div>
        </div>
      </div>
    </div>

    <script define:vars={{ apiBaseUrl }}>
      const navToggle = document.getElementById("nav-toggle");
      const primaryNav = document.getElementById("primary-navigation");

      if (navToggle && primaryNav) {
        navToggle.addEventListener("click", () => {
          const isExpanded = navToggle.getAttribute("aria-expanded") === "true";
          navToggle.setAttribute("aria-expanded", String(!isExpanded));
          primaryNav.classList.toggle("show");
        });
      }

      const apiRoot = apiBaseUrl ? apiBaseUrl.replace(/\/$/, "") : "";
      const buildApiUrl = (path) => (apiRoot ? `${apiRoot}${path}` : path);
      const summaryUrl = buildApiUrl("/api/status/summary");
      const grid = document.getElementById("status-grid");
      const modal = document.getElementById("history-modal");
      const modalTitle = document.getElementById("modal-title");
      const modalChart = document.getElementById("modal-chart");
      const modalMeta = document.getElementById("modal-meta");

      const formatTime = (epoch) => {
        if (!epoch) return "--";
        const date = new Date(epoch * 1000);
        return date.toLocaleString("en-US", { dateStyle: "medium", timeStyle: "short" });
      };

      const formatLatency = (value) => (value == null ? "--" : `${Math.round(value)} ms`);

      const formatPostsSource = (source) => {
        if (!source || source === "placeholder") {
          return "Placeholder feed";
        }
        return "Latest posts from madisonsadler.com/posts";
      };

      const renderSparkline = (el, points, color = "#8b5e3c") => {
        if (!el) return;
        const width = 120;
        const height = 40;
        el.setAttribute("viewBox", `0 0 ${width} ${height}`);
        if (!points.length) {
          el.innerHTML = `<line x1="0" y1="${height / 2}" x2="${width}" y2="${height / 2}" stroke="#d2c1ac" stroke-width="2" />`;
          return;
        }
        const values = points.map((p) => p.value);
        const min = Math.min(...values);
        const max = Math.max(...values);
        const scale = max - min || 1;
        const step = width / (points.length - 1 || 1);
        const path = points
          .map((point, index) => {
            const x = index * step;
            const y = height - ((point.value - min) / scale) * (height - 6) - 3;
            return `${index === 0 ? "M" : "L"} ${x} ${y}`;
          })
          .join(" ");
        el.innerHTML = `<path d="${path}" fill="none" stroke="${color}" stroke-width="2" stroke-linecap="round" />`;
      };

      const renderModalChart = (points) => {
        if (!points.length) {
          modalChart.innerHTML = "<text x=\"12\" y=\"60\" fill=\"#6f6153\">No history yet</text>";
          return;
        }
        const width = 300;
        const height = 120;
        const values = points.map((p) => p.value);
        const min = Math.min(...values);
        const max = Math.max(...values);
        const scale = max - min || 1;
        const step = width / (points.length - 1 || 1);
        const path = points
          .map((point, index) => {
            const x = index * step;
            const y = height - ((point.value - min) / scale) * (height - 16) - 8;
            return `${index === 0 ? "M" : "L"} ${x} ${y}`;
          })
          .join(" ");
        modalChart.setAttribute("viewBox", `0 0 ${width} ${height}`);
        modalChart.innerHTML = `<path d="${path}" fill="none" stroke="#6b4c34" stroke-width="3" stroke-linecap="round" />`;
      };

      const openModal = async (key, label) => {
        modal.classList.add("open");
        modal.setAttribute("aria-hidden", "false");
        modalTitle.textContent = `${label} history`;
        modalMeta.textContent = "Loading history…";
        modalChart.innerHTML = "";

        const response = await fetch(
          buildApiUrl(`/api/status/history?key=${encodeURIComponent(key)}&range=24h`)
        );
        if (!response.ok) {
          modalMeta.textContent = "Unable to load history.";
          return;
        }
        const data = await response.json();
        const points = (data.points || []).map((point) => {
          if (key === "uptime:site") {
            const ok = point.value.status === "ok";
            return { value: ok ? 1 : 0, time: point.recorded_at };
          }
          const value = point.value.response_ms ?? point.value.rolling_avg_ms ?? 0;
          return { value, time: point.recorded_at };
        });
        renderModalChart(points);
        modalMeta.textContent = `${points.length} points over 24 hours.`;
      };

      const closeModal = () => {
        modal.classList.remove("open");
        modal.setAttribute("aria-hidden", "true");
      };

      document.getElementById("modal-close").addEventListener("click", closeModal);
      modal.addEventListener("click", (event) => {
        if (event.target === modal) closeModal();
      });

      grid.addEventListener("click", (event) => {
        const card = event.target.closest("[data-modal-key]");
        if (!card) return;
        const key = card.dataset.modalKey;
        if (!key) return;
        const label = key === "uptime:site" ? "Uptime" : "Performance";
        openModal(key, label);
      });

      const loadSummary = async () => {
        grid.classList.add("loading");
        const response = await fetch(summaryUrl);
        grid.classList.remove("loading");
        if (!response.ok) return;
        const data = await response.json();

        const overallStatus = data.uptime?.status ?? "ok";
        const statusLabel = overallStatus === "down" ? "Down" : overallStatus === "warn" ? "Warn" : "OK";
        const overallStatusPill = document.getElementById("overall-status-pill");
        overallStatusPill.textContent = statusLabel;
        overallStatusPill.classList.remove("ok", "warn", "down");
        overallStatusPill.classList.add(overallStatus);

        const latestCheckedAt = Math.max(
          data.generated_at ?? 0,
          data.uptime?.checked_at ?? 0,
          data.perf?.checked_at ?? 0,
          data.now?.updated_at ?? 0
        );
        document.getElementById("overall-updated").textContent = formatTime(latestCheckedAt);

        const incidentCount = data.incidents?.length ?? 0;
        document.getElementById("overall-incidents").textContent =
          incidentCount === 0 ? "All clear" : `${incidentCount} active`;

        if (data.uptime) {
          const target = data.uptime.targets?.[0];
          const statusEl = document.getElementById("uptime-status");
          statusEl.textContent = data.uptime.status;
          statusEl.classList.remove("ok", "warn", "down");
          statusEl.classList.add(data.uptime.status);
          document.getElementById("uptime-latency").textContent = formatLatency(target?.latency_ms);
          document.getElementById("uptime-meta").textContent = `Last check: ${formatTime(data.uptime.checked_at)}`;
        }

        if (data.github) {
          document.getElementById("github-meta").textContent = `Recent from @${data.github.user}`;
          const list = document.getElementById("github-list");
          list.innerHTML = "";
          (data.github.items || []).slice(0, 6).forEach((item) => {
            const li = document.createElement("li");
            li.innerHTML = `<a href="${item.url}" target="_blank" rel="noreferrer">${item.repo}</a><span class=\"meta\">${item.message}</span>`;
            list.appendChild(li);
          });
        }

        if (data.posts) {
          document.getElementById("posts-meta").textContent = formatPostsSource(data.posts.source);
          const list = document.getElementById("posts-list");
          list.innerHTML = "";
          (data.posts.items || []).forEach((item) => {
            const li = document.createElement("li");
            li.innerHTML = `<a href="${item.url}" target="_blank" rel="noreferrer">${item.title}</a><span class=\"meta\">${item.published_at ? formatTime(item.published_at) : "Drafting"}</span>`;
            list.appendChild(li);
          });
        }

        if (data.now) {
          document.getElementById("now-text").textContent = data.now.text;
          document.getElementById("now-updated").textContent = `Updated ${formatTime(data.now.updated_at)}`;
        }

        if (data.incidents) {
          const container = document.getElementById("incident-log");
          container.innerHTML = "";
          if (data.incidents.length === 0) {
            container.innerHTML = "<div>All quiet on the wire.</div>";
          } else {
            data.incidents.forEach((incident) => {
              const row = document.createElement("div");
              const status = incident.ended_at ? "resolved" : incident.status;
              row.textContent = `[${status}] ${incident.target} — ${formatTime(incident.started_at)}`;
              container.appendChild(row);
            });
          }
        }

        const uptimeHistory = await fetch(buildApiUrl("/api/status/history?key=uptime:site&range=24h"));
        if (uptimeHistory.ok) {
          const history = await uptimeHistory.json();
          const points = (history.points || []).map((point) => ({
            value: point.value.status === "ok" ? 1 : 0,
          }));
          renderSparkline(document.getElementById("uptime-sparkline"), points);
        }

      };

      loadSummary();
    </script>
  </body>
</html>
